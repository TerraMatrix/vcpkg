{
  "name": "qgis",
  "version": "3.16.0",
  "port-version": 3,
  "description": "QGIS is a free, open source, cross platform (lin/win/mac) geographical information system (GIS)",
  "homepage": "https://qgis.org",
  "license": "GPL-2.0",
  "dependencies": [
    "expat",
    "hdf5",
    "libxml2",
    "libzip",
    "zlib",
    "libspatialindex",
    "libspatialite",
    "glib",
    "gdal",
    "opencl",
    "exiv2",
    "gsl",
    "libpq",
    "protobuf",
    "proj4",
    "geos",
    "poly2tri",
    {
      "name": "qt5-base",
      "default-features": false,
      "version>=": "5.15.8"
    },
    {
      "name": "qt5-svg",
      "default-features": false
    },
    "qt5-location",
    "qt5-tools",
    {
      "name": "qt5-macextras",
      "platform": "osx"
    },
    {
      "name": "qt5-winextras",
      "platform": "windows"
    },
    "qwt",
    "qca",
    "qscintilla",
    "qtkeychain",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    {
      "name": "vcpkg-get-python-packages",
      "host": true
    }
  ],
   "overrides": [
    { 
      "name": "qt5-base", 
      "version": "5.15.8",
      "port-version": 3
    }
  ],
  "features": {
    "server": {
      "description": "Determines whether QGIS server should be built",
      "dependencies": [
        "fastcgi"
      ]
    },
    "bindings": {
      "description": "Determines whether python bindings should be built",
      "dependencies": [
        "python3"
      ]
    },
    "3d": {
      "description": "Determines whether QGIS 3D library should be built",
      "dependencies": [
        "qt5-3d"
      ]
    },
    "pdal": {
      "description": "Determines whether PDAL support should be built",
      "dependencies": [
        "pdal"
      ]
    },
    "quick": {
      "description": "Determines whether QGIS Quick library should be built",
      "dependencies": [
        "qt5-declarative"
      ]
    },
    "pip-mirrors": {
      "description": "Use aliyun proxy to speed up pypi downloads in China"
    },
    "sip5": {
      "description": "Use sip5 build qgis for 3.16 version"
    },
    "debug-tools": {
      "description": "Install debug tools for qgis"
    },
    "ltr": {
      "description": "The long-term repositories for qgis"
    }
  }
}
