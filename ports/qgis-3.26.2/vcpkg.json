{
  "name": "qgis-3.26.2",
  "version": "3.26.2",
  "port-version": 0,
  "description": "QGIS is a free, open source, cross platform (lin/win/mac) geographical information system (GIS)",
  "homepage": "https://qgis.org",
  "dependencies": [
    "expat",
    "hdf5",
    "libxml2",
    "libzip",
    "zlib",
    "libspatialindex",
    "libspatialite",
    "glib",
    "gdal",
    "opencl",
    "exiv2",
    "gsl",
    "libpq",
    "protobuf",
    "proj4",
    "geos",
    "poly2tri",
    {
      "name": "qt5-base",
      "default-features": false
    },
    "qt5-svg",
    "qt5-tools",
    "qt5-location",
    {
      "name": "qt5-macextras",
      "platform": "osx"
    },
    {
      "name": "qt5-winextras",
      "platform": "windows"
    },
    "qwt",
    "qca",
    "qscintilla",
    "qtkeychain",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "features": {
    "server": {
      "description": "Determines whether QGIS server should be built",
      "dependencies": [
          "fastcgi"
      ]
    },
    "bindings": {
      "description": "Determines whether python bindings should be built",
      "dependencies": [
          "python3"
      ]
    },
    "3d": {
      "description": "Determines whether QGIS 3D library should be built",
      "dependencies": [
          "qt5-3d"
      ]
    },
    "pdal": {
      "description": "Determines whether PDAL support should be built",
      "dependencies": [
          "pdal"
      ]
    },
    "quick": {
      "description": "Determines whether QGIS Quick library should be built",
      "dependencies": [
          "qt5-declarative"
      ]
    },
    "debug-tools": {
      "description": "Install debug tools for qgis"
    },
    "pip-mirrors": {
      "description": "Use aliyun proxy to speed up pypi downloads in China"
    }
  }
}
